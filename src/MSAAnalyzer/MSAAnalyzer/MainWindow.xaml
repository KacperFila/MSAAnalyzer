                        <TextBox x:Name="EVProcedure3TextBox" Grid.Row="1" Margin="205,27,205,8" IsReadOnly="True" Width="190" Height="44" />

                        <Label Content="%EV:" Grid.Row="2" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,7,364,0" />
                        <TextBox x:Name="PercentEVProcedure3TextBox" Grid.Row="2" Margin="205,28,205,7" IsReadOnly="True" Width="190" Height="44" />

                        <ProgressBar x:Name="Procedure3PercentEVProgressBar" Grid.Row="3" Minimum="0" Maximum="40" Value="{Binding Text, ElementName=PercentEVProcedure3TextBox}" Height="44.5" Margin="205,22,205,12" Width="190" />
                        <Label Content="20%" HorizontalAlignment="Left" Grid.Row="3" Height="31" Margin="300,33,0,0" VerticalAlignment="Top" Width="106"/>

                        <Line X1="95" Y1="0" X2="95" Y2="50" Stroke="Black" StrokeThickness="2" VerticalAlignment="Top" HorizontalAlignment="Center" Width="190" Grid.Row="3" Height="43" Margin="0,23,0,0" />
                        <Line X1="142.5" Y1="0" X2="142.5" Y2="50" Stroke="Black" StrokeThickness="2" VerticalAlignment="Top" HorizontalAlignment="Center" Width="190" Margin="0,23,0,0" Height="43" Grid.Row="3" />

                        <Label Content="%EV:" Grid.Row="3" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,364,0" />
                        <TextBox x:Name="ZdolnoscEVTextBox" Grid.Column="0" Grid.Row="4" Margin="205,25,205,10" Height="44" IsReadOnly="True"/>
                        <Label Content="Zdolność systemu:" Grid.Row="4" HorizontalAlignment="Center" Height="31" VerticalAlignment="Top" Width="190" HorizontalContentAlignment="Left" Margin="0,1,0,0"/>
                        <Label Content="30%" HorizontalAlignment="Left" Grid.Row="3" Height="31" Margin="348,33,0,0" VerticalAlignment="Top" Width="106"/>
                    </Grid>

                </Grid>
            </TabItem>

            <TabItem Header="Wartości współczynników">
                <Grid VerticalAlignment="Center" Width="900">
                    <Grid.Resources>
                        <Style TargetType="TextBox">
                            <Setter Property="Effect">
                                <Setter.Value>
                                    <DropShadowEffect Color="Gray" Direction="320" ShadowDepth="2" Opacity="0.5" />
                                </Setter.Value>
                            </Setter>
                            <Setter Property="FontSize" Value="18" />
                            <Setter Property="Width" Value="700" />
                            <Setter Property="Margin" Value="10,5,0,0" />
                        </Style>
                        <Style TargetType="Label">
                            <Setter Property="FontSize" Value="25" />
                            <Setter Property="VerticalAlignment" Value="Top" />
                            <Setter Property="Margin" Value="10,10,0,0" />
                        </Style>
